@startuml
!define RECTANGLE class
!define ARROW ->

[UserController] -down-> [UserService]
[UserService] -down-> [UserRepository]
[UserService] -right-> [AddressRepository]
[UserService] -right-> [IndividualsRepository]
[UserService] -right-> [CountriesRepository]
[UserRepository] -down-> [DB]
[AddressRepository] -down-> [DB]
[IndividualsRepository] -down-> [DB]
[CountriesRepository] -down-> [DB]

@enduml